						<%} else if (game == 'wows') { %>	
							<%include wows_maps.ejs%>	
						<%} else if (game == 'blitz') { %>	
							<%include blitz_maps.ejs %>	
						<%} else if (game == 'aw') { %>	
							<%include aw_maps.ejs %>	
						<%} %>	
						</select>
					</div>

				<hr>

				<h4>Tools</h4>
				<p>
					<div id="contexts">
					<div class="btn-group">
					<button type="button" class="btn btn-default" aria-label="Left Align" id="ping" data-toggle="tooltip" title="Ping map">
						<img src="http://karellodewijk.github.io/icons/circle_icon.png" />
					</button>
					<button type="button" class="btn btn-default" aria-label="Left Align" id="icon" data-toggle="tooltip" title="Add icon (click map)">
						<img src="http://karellodewijk.github.io/icons/
							<%if (game == 'wows') { %>
								ship_icon.png
							<%} else if (game == 'aw') { %>
								aw_icon.png
							<%} else { %>
								tank_icon.png
							<%} %>
						"/>
					</button>
					<button type="button" class="btn btn-default" aria-label="Left Align" id="draw" data-toggle="tooltip" title="Free draw">
						<img src="http://karellodewijk.github.io/icons/pencil.png" />
					</button>
					<button type="button" class="btn btn-default" aria-label="Left Align" id="text" data-toggle="tooltip" title="Add Text">
						<img src="http://karellodewijk.github.io/icons/text_icon.png" />
					</button>
					<button type="button" class="btn btn-default" aria-label="Left Align" id="line" data-toggle="tooltip" title="Draw line (hold left mouse)">
