					<select name="colorpicker" id="track_colorpicker">
						<option value="#ff0000">Red</option>
						<option value="#00ff00">Green</option>
						<option value="#0000ff">Blue</option>
						<option value="#ffff00">Yellow</option>
						<option value="#ffffff">White</option>
					</select>
				
				</div>
				</div>		
				
				<div id='remove_context'>			

				<h4>Remove options</h4>
				
				<div class="container" style="max-width: 250px; padding:0px;">
				<p>
					<div class="btn-group" role="group">
						<button type="button" class="btn btn-default" aria-label="Left Align" id="clear_all" data-toggle="tooltip" title="Clear all">
							<img src="http://karellodewijk.github.io/icons/clear.png" />
						</button>
						<button type="button" class="btn btn-default" aria-label="Left Align" id="clear_draw" data-toggle="tooltip" title="Clear all free draw">
							<img src="http://karellodewijk.github.io/icons/clear_draw.png" />
						</button>
						<button type="button" class="btn btn-default" aria-label="Left Align" id="clear_icons" data-toggle="tooltip" title="clear all icons">
							<img src="http://karellodewijk.github.io/icons/clear_icon.png" />
						</button>
						<button type="button" class="btn btn-default" aria-label="Left Align" id="clear_lines" data-toggle="tooltip" title="Clear all lines and arrow">
							<img src="http://karellodewijk.github.io/icons/clear_line.png" />
						</button>
						<button type="button" class="btn btn-default" aria-label="Left Align" id="clear_text" data-toggle="tooltip" title="Clear all text">
							<img src="http://karellodewijk.github.io/icons/clear_text.png" />
						</button>
					</div>
					<br />
					<div class="btn-group" role="group">
						<button type="button" class="btn btn-default" aria-label="Left Align" id="clear_selected" data-toggle="tooltip" title="Clear all selected items">
							<img src="http://karellodewijk.github.io/icons/clear_selected.png" />
						</button>
						<button type="button" class="btn btn-default" aria-label="Left Align" id="clear_curve" data-toggle="tooltip" title="Clear all curved line">
							<img src="http://karellodewijk.github.io/icons/clear_curve.png" />
						</button>
						<button type="button" class="btn btn-default" aria-label="Left Align" id="clear_rectangle" data-toggle="tooltip" title="Clear all rectangles">
							<img src="http://karellodewijk.github.io/icons/clear_rectangle.png" />
						</button>
						<button type="button" class="btn btn-default" aria-label="Left Align" id="clear_circle" data-toggle="tooltip" title="Clear all circles">
							<img src="http://karellodewijk.github.io/icons/clear_circle.png" />
						</button>
						<button type="button" class="btn btn-default" aria-label="Left Align" id="clear_polygon" data-toggle="tooltip" title="Clear all polygons">
							<img src="http://karellodewijk.github.io/icons/clear_polygon.png" />
						</button>
					</div>
					<br />
					<div class="btn-group" role="group">
						<button type="button" class="btn btn-default" aria-label="Left Align" id="clear_area" data-toggle="tooltip" title="Clear all areas">
							<img src="http://karellodewijk.github.io/icons/clear_area.png" />
						</button>
						
					</div>
				
				</div>
				</div>

				<div id='text_context'>

				<h4>Text options</h4>
				
				<div class="container" style="max-width: 250px; padding:0px;">
				<p>	
				<small>Text:</small> <input type="text" id="text_tool_text"/>
					<p>
					<select name="colorpicker" id="text_colorpicker">
						<option value="#ffffff">White</option>
						<option value="#ff0000">Red</option>
						<option value="#00ff00">Green</option>
						<option value="#0000ff">Blue</option>
						<option value="#ffff00">Yellow</option>
						<option value="#000000">Black</option>
					</select>
					
					<br />
					<div class="row">
						<div class="col-sm-3">
							<small>Size</small>
						</div>
						<div class="col-sm-5">
							<input id="font_size" class="my_sliders" data-slider-id='ex1Slider' type="text" data-slider-min="5" data-slider-max="99" data-slider-step="1" data-slider-value="10"/>
						</div>
						<div class="col-sm-3">
							<input id="font_size_text" class="slider_text_input" type="text"/>
						</div>
					</div>
				</div>
				</div>

				<div id='line_context'>	
								
				<h4>Line options</h4>
				
				<div class="container" style="max-width: 250px; padding:0px;">
				<p>
					<select name="colorpicker" id="line_colorpicker">
						<option value="#ff0000">Red</option>
						<option value="#00ff00">Green</option>
						<option value="#0000ff">Blue</option>
						<option value="#ffff00">Yellow</option>
						<option value="#ffffff">White</option>
					</select>
					<br/>
					<div class="row">
						<div class="col-sm-3">
							<small>Thickness</small>
						</div>
						<div class="col-sm-5">
							<input id="line_thickness" class="my_sliders" data-slider-id='ex1Slider' type="text" data-slider-min="1" data-slider-max="10" data-slider-step="1" data-slider-value="2"/>
						</div>
						<div class="col-sm-3">
							<input id="line_thickness_text" class="slider_text_input" type="text"/>
						</div>
					</div>
					<p>
					<div class="btn-group" role="group" id="line_type">
						<button type="button" class="btn btn-default" aria-label="Left Align" id="full_line">
							<img src="http://karellodewijk.github.io/icons/line_icon.png" />
						</button>
						<button type="button" class="btn btn-default" aria-label="Left Align" id="dotted_line">
							<img src="http://karellodewijk.github.io/icons/dotted_line.png" />
						</button>
						<button type="button" class="btn btn-default" aria-label="Left Align" id="arrow">
							<img src="http://karellodewijk.github.io/icons/arrow.png" />
						</button>
						<button type="button" class="btn btn-default" aria-label="Left Align" id="dotted_arrow">
							<img src="http://karellodewijk.github.io/icons/dotted_arrow.png" />
						</button>
					</div>
				</div>
				</div>

				<%if (game == 'blitz') { %>	
				<h4>Extra</h4>
				<p>
				
				<div class="btn-group" role="group">
				<a id="save" href="javascript:void(0)" class="btn btn-default" data-placement="bottom" data-toggle="tooltip" title="Save">
					<img src="http://karellodewijk.github.io/icons/save.png">
				</a>
				<a id="save_as" href="javascript:void(0)" class="btn btn-default" data-placement="bottom"
				data-toggle="popover" data-content="<input type='text' id='tactic_name' style='width:100px' /> <button id='store_tactic' class='btn btn-default btn-xs'>Store</button>">
					<img data-toggle="tooltip" title="Save as" src="http://karellodewijk.github.io/icons/save_as.png">
				</a>

				<button id="lock" class="btn btn-default" data-toggle="tooltip" title="Locks/unlocks edit mode for users without additional permissions">
					<img src="http://karellodewijk.github.io/icons/lock.png">
				</button>
				<button id="export" class="btn btn-default" data-toggle="tooltip" title="Save tactic as image">
					<img src="http://karellodewijk.github.io/icons/export.png">
				</button>
				<button id="link" class="btn btn-default" data-toggle="tooltip" title="Copy link to clipboard, send it to friends or clan mates to invite them to this tactic">
					<img src="http://karellodewijk.github.io/icons/link.png">
				</button>
				</div>
				<% } %>	
				
			</div>
			<%if (game != 'blitz') { %>	
			<div class="right_column">
				<h4>User list (<span id="user_count"></span>)</h4>
				<p>
				<div class="btn-group-vertical btn-group-xs" role="group" id="userlist"></div>
			</div>
			<% } %>	
		</div>
		<%if (game != 'blitz') { %>	
		<div class="bottom">
			<textarea readonly id="chat_box" rows="10" cols="60"></textarea>
			<br />
			<input id="chat_input" class="slider_text_input" type="text"/>
		</div>
		<% } %>	
	</div>

<%if (game == 'blitz') { %>	
	<div class="side_bar jumbotron">
		<div class="bottom">
			<textarea readonly id="chat_box" rows="10" cols="60"></textarea>
			<br />
			<input id="chat_input" class="slider_text_input" type="text"/>
		</div>
	</div>
	<div class="side_bar jumbotron">
		<div class="right_column">
			<h4>User list (<span id="user_count"></span>)</h4>
			<p>
			<div class="btn-group-vertical btn-group-xs" role="group" id="userlist"></div>
		</div>
	</div>
<% } %>	
	
	
</body>